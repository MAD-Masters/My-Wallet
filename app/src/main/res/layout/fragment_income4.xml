<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UI.Income.Income4">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="235dp"
            android:layout_height="169dp"
            android:layout_marginTop="48dp"
            android:src="@drawable/wallet_with_mens"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.431"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/update"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_marginTop="60dp"
            android:background="@drawable/background_blue_fully_rounded"

            android:text="@string/add"
            android:textColor="#ffff"
            app:layout_constraintEnd_toStartOf="@+id/cansel2"
            app:layout_constraintHorizontal_bias="0.558"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textInputEditText3" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/cansel2"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_marginTop="60dp"
            android:background="@drawable/background_blue_fully_rounded"
            android:backgroundTint="@color/colorPink"
            android:text="@string/goal12"
            android:textColor="#ffff"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.749"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textInputEditText3" />

        <EditText
            android:id="@+id/textInputEditText8"
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:layout_marginTop="48dp"
            android:background="@drawable/background_blue_no_fills"
            android:hint="Wallet Name"
            android:textColor="@color/primaryColor"
            android:lines="1"
            android:maxLength="30"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.433"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView11" />

        <EditText
            android:id="@+id/textInputEditText3"
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:layout_marginTop="28dp"
            android:textColor="@color/primaryColor"
            android:background="@drawable/background_blue_no_fills"
            android:hint="Bank"
            android:lines="1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.433"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textInputEditText8" />



    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>